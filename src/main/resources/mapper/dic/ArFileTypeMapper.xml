<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haiyu.manager.dao.dic.ArFileTypeMapper">
    <resultMap id="BaseResultMap" type="com.haiyu.manager.pojo.dic.ArFileTypeDO">
        <!--
          WARNING - @mbg.generated
        -->
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="file_type_id" jdbcType="VARCHAR" property="fileTypeId" />
        <result column="code" jdbcType="VARCHAR" property="code" />
        <result column="description" jdbcType="VARCHAR" property="description" />
        <result column="logic_delete" jdbcType="VARCHAR" property="logicDelete" />
        <result column="create_time" jdbcType="VARCHAR" property="createTime" />
        <result column="device_name" jdbcType="VARCHAR" property="deviceName" />
    </resultMap>

    <update id="updateArFileType">
        UPDATE dic_ar_file_type
        <set>
            <if test="fileTypeId != null">
                file_type_id = #{fileTypeId},
            </if>
            <if test="code != null">
                code = #{code},
            </if>
            <if test="description != null">
                description = #{description},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="deviceName != null">
                device_name = #{deviceName}
            </if>
        </set>
        WHERE id = #{id}
    </update>

    <sql id="baseColumn">
    id,file_type_id,code,description,logic_delete,create_time,device_name
  </sql>

    <select id="getArFileTypeList" resultType="com.haiyu.manager.pojo.dic.ArFileTypeDO">
        SELECT <include refid="baseColumn"/>
        FROM dic_ar_file_type
    </select>

</mapper>